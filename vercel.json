{
    "version": 2,
    "name": "merchant-backend",
    "functions": {
      "server.js": {
        "runtime": "@vercel/node@18.x",
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }